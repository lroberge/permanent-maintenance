config_version=5

[addons]

resources_spreadsheet_view/color_rows=true
resources_spreadsheet_view/color_arrays=true
resources_spreadsheet_view/array_min_width=128.0
resources_spreadsheet_view/resource_preview_size=32.0
resources_spreadsheet_view/clip_headers=false
resources_spreadsheet_view/dupe_arrays=true
resources_spreadsheet_view/context_menu_on_leftclick=false

[application]

config/name="playoffline"
run/main_scene="res://debug room.tscn"
config/features=PackedStringArray("4.1", "Forward Plus")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
config/icon="res://assets/external/netneighborhood.png"
config/windows_native_icon="res://assets/external/net.ico"

[autoload]

ActionsCoord="*res://player_actions_coordinator.gd"
CursorInputManager="*res://cursor_input_manager.gd"
AoeManager="*res://aoe_manager.gd"
EncounterManager="*res://encounter_manager.gd"
AudioManager="*res://BgAudioManager.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
Chat="*res://chat_dialogue_coordinator.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/initial_position_type=2
window/size/initial_screen=1
window/size/window_width_override=1152
window/size/window_height_override=648
window/energy_saving/keep_screen_on=false
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/dpi/allow_hidpi=false

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg", "res://addons/godot_state_charts/plugin.cfg", "res://addons/jamcountdown/plugin.cfg", "res://addons/resources_spreadsheet_view/plugin.cfg")

